{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "Umbraco.Core/Umbraco.Core.csproj",
            "Umbraco.Examine.Lucene/Umbraco.Examine.Lucene.csproj",
            "Umbraco.Persistence.SqlCe/Umbraco.Persistence.SqlCe.csproj",
            "Umbraco.PublishedCache.NuCache/Umbraco.PublishedCache.NuCache.csproj",
            "Umbraco.Web.BackOffice/Umbraco.Web.BackOffice.csproj",
            "Umbraco.Web.Common/Umbraco.Web.Common.csproj",
            "Umbraco.Web.Website/Umbraco.Web.Website.csproj",
            "Umbraco.Infrastructure/Umbraco.Infrastructure.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",

            "**/Umbraco.Infrastructure/Cache/**",
            "**/Umbraco.Infrastructure/Compose/**",
            "**/Umbraco.Infrastructure/Configuration/**",
            "**/Umbraco.Infrastructure/DependencyInjection/**",
            "**/Umbraco.Infrastructure/Deploy/**",
            "**/Umbraco.Infrastructure/Events/**",
            "**/Umbraco.Infrastructure/Examine/**",

            "**/Umbraco.Infrastructure/Extensions/**",
            "**/Umbraco.Infrastructure/HealthChecks/**",
            "**/Umbraco.Infrastructure/HostedServices/**",
            "**/Umbraco.Infrastructure/Install/**",
            "**/Umbraco.Infrastructure/Logging/**",
            "**/Umbraco.Infrastructure/Macros/**",
            "**/Umbraco.Infrastructure/Manifest/**",
            "**/Umbraco.Infrastructure/Media/**",
            "**/Umbraco.Infrastructure/Migrations/**",


            "**/Umbraco.Infrastructure/Persistence/**",
            "**/Umbraco.Infrastructure/Services/**",


            "**/Umbraco.Infrastructure/PropertyEditors/**"

          ]
        }
      ],
      "dest": "api",
      "filter": "docfx.filter.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Umbraco c# Api docs",
      "_enableSearch": true,
      "_disableContribution": false
    },
    "dest": "_site",
    "template": [
      "default", "umbracotemplate"
    ]
  }
}
