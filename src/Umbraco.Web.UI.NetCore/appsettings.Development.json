{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information"
        },
        "WriteTo": [
            {
                "Name": "Async",
                "Args": {
                    "configure": [
                        {
                            "Name": "Console"
                        }
                    ]
                }
            }
        ]
    },
    "Umbraco": {
      "CMS": {
        "Global": {
          "Smtp": {
            //"From": "your@email.here",
            //"Host": "localhost",
            // "Port": "25"
          }
        },
        "Hosting": {
          "Debug": true
        },
        "RuntimeMinification": {
          "useInMemoryCache": true,
          "cacheBuster": "Timestamp"
        },
        "RichTextEditor": {
          "Commands": [
            {
              "Alias": "fullscreen",
              "Name": "Full Screen",
              "Mode": "All"
            }
          ],
          "Plugins": [
            "fullscreen"
          ]
        }
      }

    }
}
