        "type": "object",
        "additionalProperties": false,
        "properties": {
          "serverStatus": {
            "$ref": "#/components/schemas/RuntimeLevel"
          }
        }
      },
      "RuntimeLevel": {
        "type": "string",
        "description": "Describes the levels in which the runtime can run.\n            ",
        "x-enumNames": [
          "Unknown",
          "Boot",
          "Install",
          "Upgrade",
          "Run",
          "BootFailed"
        ],
        "enum": [
          "Unknown",
          "Boot",
          "Install",
          "Upgrade",
          "Run",
          "BootFailed"
        ]
