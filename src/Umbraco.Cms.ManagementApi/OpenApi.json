        "properties": {
          "id": {
            "type": "string",
            "format": "guid",
            "minLength": 1
          },
          "providerName": {
            "type": "string",
            "minLength": 1
          },
          "server": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
