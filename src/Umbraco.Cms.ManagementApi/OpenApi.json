        "type": "object",
        "additionalProperties": false,
        "required": [
          "user",
          "database"
        ],
        "properties": {
          "user": {
            "$ref": "#/components/schemas/UserInstallViewModel"
          },
          "database": {
            "$ref": "#/components/schemas/DatabaseInstallViewModel"
          },
          "telemetryLevel": {
            "$ref": "#/components/schemas/TelemetryLevel"
          }
        }
      },
      "UserInstallViewModel": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "name",
          "email",
          "password"
        ],
        "properties": {
          "name": {
            "type": "string",
            "maxLength": 255,
            "minLength": 0
          },
          "email": {
            "type": "string",
            "format": "email",
            "minLength": 1
          },
          "password": {
            "type": "string",
            "minLength": 1
          },
          "subscribeToNewsletter": {
            "type": "boolean"
          }
        }
