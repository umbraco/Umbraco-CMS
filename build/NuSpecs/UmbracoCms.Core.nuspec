    <version>8.0.0</version>
    <title>Umbraco Cms Core Binaries</title>
    <authors>Umbraco HQ</authors>
    <owners>Umbraco HQ</owners>
    <licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
